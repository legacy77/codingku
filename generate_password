<?php

 $validCharacters = "abcdefghijklmnopqrstuxyvwzABCDEFGHIJKLMNOPQRSTUXYVWZ1234567890";
 $validCharNumber = strlen($validCharacters);
 $passwd = "";
                for ($i = 0; $i < 10; $i++) {
                    $index = mt_rand(0, $validCharNumber - 1);
                    $passwd .= $validCharacters[$index];
                }
  echo $passwd;
